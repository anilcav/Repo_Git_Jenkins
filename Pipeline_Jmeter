pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git branch: 'https://github.com/anilcav/Repo_Git_Jenkins.git', credentialsId: 'your_credentials', url: 'your_git_url'

            }
        }
    }
    stage('Run JMeter Test') {
        steps {
            dir('D:\Data\Jmeter') {
                bat 'D:\Data\Jmeter\bin\jmeter -n -t JpetStore.jmx -l result.jtl'
            }
        }
    }
}
